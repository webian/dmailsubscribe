{
  "name": "dpn/dmailsubscribe",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension Dmailsubscribe: Plugin for direct_mail subscriptions",
  "keywords": [
    "TYPO3",
    "extension",
    "direct_mail",
    "subscription"
  ],
  "homepage": "https://github.com/bjo3rnf/dmailsubscribe",
  "authors": [
    {
      "name": "BjÃ¶rn Fromme",
      "role": "Developer",
      "homepage": "https://dreipunktnull.com"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=7.6.0,<8.0"
  },
  "require-dev": {
    "mikey179/vfsStream": "1.4.*@dev",
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "DPN\\Dmailsubscribe\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DPN\\Dmailsubscribe\\Tests\\": "Tests",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
      "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
    }
  },
  "replace": {
    "dmailsubscribe": "self.version",
    "typo3-ter/dmailsubscribe": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
